/**
 *
 * @author elsyj
 */
public abstract class Perro extends Animal implements Mascota {
    public Perro(String nombre, int edad){
        super(nombre, edad);
      
    }
    public void hacerSonido(){
        System.out.println(getNombre()+"dice: ¡Guau guau!");
    }
    public void moverse(){
        System.out.println(getNombre()+"corre rapidamente");
    }
    public void jugar(){
        System.out.println(getNombre() + "juega con la pelota.");
    }
    
}
