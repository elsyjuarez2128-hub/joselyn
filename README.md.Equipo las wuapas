/**
 *
 * @author elsyj
 */
public class Mian {
    public static void main(String[] args) {
        Perro perro =new Perro("Esmogli",4){};
        Gato gato = new Gato("Michi",2) {};
        Delfin delfin = new Delfin("Ariel",6){};
        
    perro.hacerSonido();
    perro.moverse();
    perro.jugar();
    
    
    gato.hacerSonido();
    gato.moverse();
    gato.jugar();
    
    
    delfin.hacerSonido();
    delfin.moverse();
    
    }  
}


      
  
          
          
    
  
    



