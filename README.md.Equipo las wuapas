/**
 *
 * @author elsyj
 */
public class Main {
    public static void main(String[] args) {
        Delfin miDelfin;
        miDelfin = new Delfin("Ariel",4) {};
    
    miDelfin.hacerSonido();
    miDelfin.moverse();
    
    
}
}

